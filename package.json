    "start": "node_modules/.bin/webpack-dev-server --progress",
    "build": "cross-env NODE_ENV=production UV_THREADPOOL_SIZE=100 webpack --config ./webpack.config",
    "deploy": "gh-pages -d build",
    "lint": "eslint src"
  },
